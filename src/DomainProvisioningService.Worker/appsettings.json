{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "CertificateStore": "Host=localhost;Port=5432;Database=certificate_store;Username=postgres;Password="
  },
  "CabinetApi": {
    "BaseUrl": "http://localhost:5000"
  },
  "HAProxyAgent": {
    "BaseUrl": "http://localhost:8081"
  },
  "Acme": {
    "UseStaging": true,
    "Email": "admin@example.com"
  },
  "Renewal": {
    "PollIntervalHours": 6,
    "ThresholdDays": 30
  }
}
